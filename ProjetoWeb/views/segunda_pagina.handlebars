<div class="container md">
        <h5 style="margin-top: 1.5rem;">Consulta - Potenciais Clientes</h5>
        <p>Lista de agendamentos feitos</p>
    <br><br>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nome</th>
            <th scope="col">Telefone</th>
            <th scope="col">Origem</th>
            <th scope="col">Data</th>
            <th scope="col">Observação</th>
            <th scope="col">Ações</th>
        </tr>
    </thead>
    <tbody>
        {{#each post}}
        {{#dataValues}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{nome}}</td>
            <td>{{telefone}}</td>
            <td>{{origem}}</td>
            <td>{{data}}</td>
            <td>{{observacao}}</td>
            <td><a href="/excluir/{{id}}" class="btn btn-outline-primary">Excluir</a></td>
        </tr>
        {{/dataValues}}
        {{/each}}
    </tbody>
</table>
</div>